{
	"manifest_version": 2,
	"name": "SpeechWire Edit",
	"description": "Edit SpeechWire Judging Comments Page",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon_small.png",
		"default_popup": "popup/index.html",
		"default_title": "Open menu"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}